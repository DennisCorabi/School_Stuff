<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="450.0" prefWidth="499.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.example.eserciziojavafx.RegisterController">
   <TextArea fx:id="registerTextArea" layoutX="89.0" layoutY="54.0" prefHeight="276.0" prefWidth="330.0"/>
   <Label layoutX="203.0" layoutY="14.0" prefHeight="40.0" prefWidth="93.0" text="Rubrica">
      <font>
         <Font size="27.0"/>
      </font>
   </Label>
   <Button fx:id="showContactsButton" layoutX="303.0" layoutY="343.0" mnemonicParsing="false" onAction="#ShowContacts"
           prefHeight="25.0" prefWidth="116.0" text="Mostra contatti"/>
   <TextField fx:id="searchField" layoutX="89.0" layoutY="343.0" prefHeight="25.0" prefWidth="102.0"
              promptText="numero"/>
   <Button fx:id="searchButton" layoutX="206.0" layoutY="343.0" mnemonicParsing="false" onAction="#searchPerson"
           prefHeight="25.0" prefWidth="66.0" text="Cerca"/>
   <TextField fx:id="deleteField" layoutX="89.0" layoutY="385.0" prefHeight="25.0" prefWidth="102.0"
              promptText="numero"/>
   <Button fx:id="deletePersonButton" layoutX="206.0" layoutY="385.0" mnemonicParsing="false" onAction="#DeletePerson"
           prefHeight="25.0" prefWidth="66.0" text="Elimina"/>
</AnchorPane>
